<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RoomSetUpActivity"
    android:orientation="vertical">
    <include layout="@layout/activity_room_set_up_toolbar"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Oda İsmini Giriniz"
        android:textAlignment="center"
        android:id="@+id/activity_room_set_up_roomname"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Odayı Kur"
        android:textSize="20dp"
        android:id="@+id/activity_room_set_up_roomsetup"/>

</LinearLayout>